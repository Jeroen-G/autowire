{
    "name": "jeroen-g/autowire",
    "description": "Autowire and configure using PHP 8 Attributes in Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen",
            "email": "jeroengjeroeng@gmail.com",
            "homepage": "https://jeroeng.dev"
        }
    ],
    "homepage": "https://github.com/jeroen-g/autowire",
    "keywords": ["Laravel", "autowire"],
    "require": {
        "php": "8.0.*||8.1.*",
        "ergebnis/classy": "^1.3",
        "illuminate/support": "~8.0||~9.0",
        "illuminate/console": "~8.0||~9.0",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "mockery/mockery": "^1.4",
        "infection/infection": "^0.25",
        "symplify/easy-coding-standard": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "JeroenG\\Autowire\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JeroenG\\Autowire\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JeroenG\\Autowire\\AutowireServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
